#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/huanglei/workspace/my-cs-self-learning/node_modules/.pnpm/vuepress@2.0.0-rc.19_@vuepress+bundler-vite@2.0.0-rc.19_@types+node@22.10.7__vue@3.5.13/node_modules/vuepress/bin/node_modules:/home/huanglei/workspace/my-cs-self-learning/node_modules/.pnpm/vuepress@2.0.0-rc.19_@vuepress+bundler-vite@2.0.0-rc.19_@types+node@22.10.7__vue@3.5.13/node_modules/vuepress/node_modules:/home/huanglei/workspace/my-cs-self-learning/node_modules/.pnpm/vuepress@2.0.0-rc.19_@vuepress+bundler-vite@2.0.0-rc.19_@types+node@22.10.7__vue@3.5.13/node_modules:/home/huanglei/workspace/my-cs-self-learning/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/huanglei/workspace/my-cs-self-learning/node_modules/.pnpm/vuepress@2.0.0-rc.19_@vuepress+bundler-vite@2.0.0-rc.19_@types+node@22.10.7__vue@3.5.13/node_modules/vuepress/bin/node_modules:/home/huanglei/workspace/my-cs-self-learning/node_modules/.pnpm/vuepress@2.0.0-rc.19_@vuepress+bundler-vite@2.0.0-rc.19_@types+node@22.10.7__vue@3.5.13/node_modules/vuepress/node_modules:/home/huanglei/workspace/my-cs-self-learning/node_modules/.pnpm/vuepress@2.0.0-rc.19_@vuepress+bundler-vite@2.0.0-rc.19_@types+node@22.10.7__vue@3.5.13/node_modules:/home/huanglei/workspace/my-cs-self-learning/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vuepress@2.0.0-rc.19_@vuepress+bundler-vite@2.0.0-rc.19_@types+node@22.10.7__vue@3.5.13/node_modules/vuepress/bin/vuepress-webpack.js" "$@"
else
  exec node  "$basedir/../../../../../../vuepress@2.0.0-rc.19_@vuepress+bundler-vite@2.0.0-rc.19_@types+node@22.10.7__vue@3.5.13/node_modules/vuepress/bin/vuepress-webpack.js" "$@"
fi
